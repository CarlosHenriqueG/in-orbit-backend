#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/drizzle-kit@0.24.2/node_modules/drizzle-kit/node_modules:/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/drizzle-kit@0.24.2/node_modules:/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/drizzle-kit@0.24.2/node_modules/drizzle-kit/node_modules:/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/drizzle-kit@0.24.2/node_modules:/home/carlos/Documentos/Dev/projects/ionorbit/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../drizzle-kit/bin.cjs" "$@"
else
  exec node  "$basedir/../drizzle-kit/bin.cjs" "$@"
fi
